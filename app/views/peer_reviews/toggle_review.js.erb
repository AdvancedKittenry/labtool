$("<%=@selector%>").val("<%=@label%>")

$("<%=@class_selector%>").removeClass();
$("<%=@student_selector%>").removeClass();
$("<%=@review_target_selector%>").removeClass();

<% if @label=="cancel" %>
  $("<%=@class_selector%>").addClass("cancel");
  $("<%=@student_selector%>").addClass("<%=@student_class%>");
  $("<%=@review_target_selector%>").addClass("rotate <%=@student_class%>");
<% else %>
  $("<%=@class_selector%>").addClass("review");
  $("<%=@review_target_selector%>").addClass("rotate");
<% end %>


