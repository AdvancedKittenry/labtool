<h1>Listing week_feedbacks</h1>

<table>
  <tr>
    <th>Points</th>
    <th>Text</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @week_feedbacks.each do |week_feedback| %>
  <tr>
    <td><%= week_feedback.points %></td>
    <td><%= week_feedback.text %></td>
    <td><%= link_to 'Show', week_feedback %></td>
    <td><%= link_to 'Edit', edit_week_feedback_path(week_feedback) %></td>
    <td><%= link_to 'Destroy', week_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Week feedback', new_week_feedback_path %>
