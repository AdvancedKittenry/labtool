$("<%=@selector%>").val("<%=@label%>")

$("<%=@class_selector%>").removeClass();
$("<%=@student_selector%>").removeClass();
$("<%=@review_target_selector%>").removeClass();

$("<%=@student_selector%>").addClass("<%=@reviewer_class%>");
$("<%=@review_target_selector%>").addClass("vertical <%=@reviewed_class%>");

<% if @label=="cancel" %>
  $("<%=@class_selector%>").addClass("cancel");
<% else %>
  $("<%=@class_selector%>").addClass("review");
<% end %>


